# Azure Cosmos DB Configuration
# Add these environment variables to your .env.local file

# Azure Cosmos DB Gremlin API Configuration
COSMOS_DB_ENDPOINT=your-cosmos-db-endpoint.documents.azure.com
COSMOS_DB_KEY=your-cosmos-db-primary-key
COSMOS_DB_DATABASE=councilworks
COSMOS_DB_GRAPH=assets

# Optional: Cosmos DB Connection Settings
COSMOS_DB_CONNECTION_MODE=Gateway
COSMOS_DB_MAX_CONNECTION_POOL_SIZE=10
COSMOS_DB_REQUEST_TIMEOUT=30000

# Development/Testing
COSMOS_DB_ENDPOINT_DEV=your-dev-cosmos-db-endpoint.documents.azure.com
COSMOS_DB_KEY_DEV=your-dev-cosmos-db-key
COSMOS_DB_DATABASE_DEV=councilworks-dev
COSMOS_DB_GRAPH_DEV=assets-dev

# Production
COSMOS_DB_ENDPOINT_PROD=your-prod-cosmos-db-endpoint.documents.azure.com
COSMOS_DB_KEY_PROD=your-prod-cosmos-db-key
COSMOS_DB_DATABASE_PROD=councilworks-prod
COSMOS_DB_GRAPH_PROD=assets-prod
